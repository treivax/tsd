{
  "parser": {
    "max_expressions": 2000,
    "debug": false,
    "recover": true
  },
  "validator": {
    "strict_mode": true,
    "allowed_operators": [
      "==",
      "!=",
      "<",
      ">",
      "<=",
      ">=",
      "AND",
      "OR",
      "NOT",
      "+",
      "-",
      "*",
      "/",
      "%"
    ],
    "max_depth": 25
  },
  "logger": {
    "level": "debug",
    "format": "json",
    "output": "stdout"
  },
  "debug": false,
  "version": "1.0.0"
}
