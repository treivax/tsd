// TESTS EXHAUSTIFS DES NŒUDS ALPHA - COUVERTURE COMPLÈTE
// Tous opérateurs, types, succès/échecs, combinaisons logiques

// Types étendus pour tests exhaustifs
type TestPerson : <id: string, name: string, age: number, salary: number, active: bool, score: number, tags: string, status: string>
type TestProduct : <id: string, name: string, price: number, category: string, available: bool, rating: number, keywords: string, brand: string>

// ===============================================================
// PARTIE 1 : TESTS DE COMPARAISON BASIQUE (SUCCESS + FAILURE)
// ===============================================================

// === ÉGALITÉ - SUCCESS ===
{p: TestPerson} / p.name == "Alice" ==> equality_string_success(p.name)
{p: TestPerson} / p.age == 25 ==> equality_number_success(p.name)
{p: TestPerson} / p.active == true ==> equality_boolean_success(p.name)
{p: TestPerson} / p.score == 87.5 ==> equality_decimal_success(p.name)

// === ÉGALITÉ - FAILURE (inverse des valeurs précédentes) ===
{p: TestPerson} / p.name == "INVALID" ==> equality_string_failure(p.name)
{p: TestPerson} / p.age == 999 ==> equality_number_failure(p.name)
{p: TestPerson} / p.active == false ==> equality_boolean_failure(p.name)
{p: TestPerson} / p.score == 999.99 ==> equality_decimal_failure(p.name)

// === INÉGALITÉ - SUCCESS ===
{p: TestPerson} / p.name != "INVALID" ==> inequality_string_success(p.name)
{p: TestPerson} / p.age != 999 ==> inequality_number_success(p.name)
{p: TestPerson} / p.active != false ==> inequality_boolean_success(p.name)
{p: TestPerson} / p.score != 999.99 ==> inequality_decimal_success(p.name)

// === INÉGALITÉ - FAILURE ===
{p: TestPerson} / p.name != "Alice" ==> inequality_string_failure(p.name)
{p: TestPerson} / p.age != 25 ==> inequality_number_failure(p.name)
{p: TestPerson} / p.active != true ==> inequality_boolean_failure(p.name)
{p: TestPerson} / p.score != 87.5 ==> inequality_decimal_failure(p.name)

// === COMPARAISONS NUMÉRIQUES - SUCCESS ===
{p: TestPerson} / p.age > 20 ==> greater_than_success(p.name)
{p: TestPerson} / p.age >= 25 ==> greater_equal_success(p.name)
{p: TestPerson} / p.age < 30 ==> less_than_success(p.name)
{p: TestPerson} / p.age <= 25 ==> less_equal_success(p.name)

// === COMPARAISONS NUMÉRIQUES - FAILURE ===
{p: TestPerson} / p.age > 30 ==> greater_than_failure(p.name)
{p: TestPerson} / p.age >= 30 ==> greater_equal_failure(p.name)
{p: TestPerson} / p.age < 20 ==> less_than_failure(p.name)
{p: TestPerson} / p.age <= 20 ==> less_equal_failure(p.name)

// ===============================================================
// PARTIE 2 : OPÉRATEURS DE PATTERN (si supportés par l'évaluateur)
// ===============================================================

// === PATTERN MATCHING ===
// Note: Ces tests échoueront probablement car l'évaluateur ne supporte pas IN/LIKE/etc
// Mais ils permettent de tester la robustesse du système

// Test IN
{p: TestPerson} / p.status IN ["ACTIVE", "PENDING"] ==> pattern_in_success(p.name)

// Test CONTAINS (si supporté)
{p: TestPerson} / p.tags CONTAINS "admin" ==> pattern_contains_success(p.name)

// Test LIKE (si supporté)
{p: TestPerson} / p.name LIKE "A%" ==> pattern_like_success(p.name)

// ===============================================================
// PARTIE 3 : COMBINAISONS LOGIQUES ALPHA COMPLEXES
// ===============================================================

// === TESTS AND SIMPLES ===
{p: TestPerson} / p.age >= 18 AND p.active == true ==> logical_and_adult_active_success(p.name)
{p: TestPerson} / p.salary > 40000 AND p.score > 80.0 ==> logical_and_salary_score_success(p.name)
{p: TestPerson} / p.name == "Alice" AND p.age == 25 ==> logical_and_exact_match_success(p.name)

// === TESTS AND FAILURE ===
{p: TestPerson} / p.age >= 65 AND p.active == true ==> logical_and_senior_active_failure(p.name)
{p: TestPerson} / p.salary < 30000 AND p.score > 90.0 ==> logical_and_lowsal_hiscore_failure(p.name)

// === TESTS OR ===
{p: TestPerson} / p.age < 18 OR p.age > 65 ==> logical_or_minor_senior_success(p.name)
{p: TestPerson} / p.salary > 100000 OR p.score > 95.0 ==> logical_or_rich_expert_success(p.name)
{p: TestPerson} / p.name == "Alice" OR p.name == "Bob" ==> logical_or_specific_names_success(p.name)

// === TESTS OR FAILURE ===
{p: TestPerson} / p.age < 18 OR p.age > 65 ==> logical_or_minor_senior_failure(p.name)
{p: TestPerson} / p.salary > 200000 OR p.score > 99.0 ==> logical_or_extreme_values_failure(p.name)

// === TESTS NOT ===
{p: TestPerson} / NOT (p.age < 18) ==> logical_not_minor_success(p.name)
{p: TestPerson} / NOT (p.active == false) ==> logical_not_inactive_success(p.name)
{p: TestPerson} / NOT (p.salary > 100000) ==> logical_not_rich_success(p.name)

// === TESTS NOT FAILURE ===
{p: TestPerson} / NOT (p.age >= 18) ==> logical_not_adult_failure(p.name)
{p: TestPerson} / NOT (p.active == true) ==> logical_not_active_failure(p.name)

// === COMBINAISONS COMPLEXES ===
{p: TestPerson} / (p.age >= 18 AND p.age <= 65) AND (p.active == true OR p.score > 90.0) ==> complex_working_age_good_profile_success(p.name)
{p: TestPerson} / (p.salary > 50000 OR p.score > 85.0) AND NOT (p.status == "DISABLED") ==> complex_qualified_enabled_success(p.name)

// ===============================================================
// PARTIE 4 : CAS LIMITES ET VALEURS SPÉCIALES
// ===============================================================

// === VALEURS LIMITES ===
{p: TestPerson} / p.age == 0 ==> edge_case_zero_age_success(p.name)
{p: TestPerson} / p.salary == -1000 ==> edge_case_negative_salary_success(p.name)
{p: TestPerson} / p.name == "" ==> edge_case_empty_string_success(p.name)
{p: TestPerson} / p.score == 0.0 ==> edge_case_zero_score_success(p.name)

// === PRÉCISION DÉCIMALE ===
{p: TestPerson} / p.score == 87.50000 ==> precision_decimal_exact_success(p.name)
{prod: TestProduct} / prod.rating == 3.14159265 ==> precision_pi_exact_success(prod.name)

// === COMPARAISONS STRING AVANCÉES ===
{p: TestPerson} / p.name < "B" ==> string_comparison_less_success(p.name)
{p: TestPerson} / p.name > "Z" ==> string_comparison_greater_failure(p.name)

// === TESTS PRODUIT POUR DIVERSITÉ ===
{prod: TestProduct} / prod.available == true AND prod.price < 100.0 ==> product_affordable_available_success(prod.name)
{prod: TestProduct} / prod.category == "Electronics" OR prod.category == "Books" ==> product_main_categories_success(prod.name)
{prod: TestProduct} / NOT (prod.price > 1000.0) ==> product_not_expensive_success(prod.name)

// ===============================================================
// PARTIE 5 : STRESS TESTS AVEC MULTIPLES CHAMPS
// ===============================================================

// === TOUS LES CHAMPS ===
{p: TestPerson} / p.name == "Alice" AND p.age == 25 AND p.salary == 45000 AND p.active == true AND p.score == 87.5 ==> all_fields_exact_match_success(p.name)

// === PROFILS MÉTIER ===
{p: TestPerson} / p.age >= 25 AND p.age <= 45 AND p.salary >= 50000 AND p.active == true ==> profile_prime_worker_success(p.name)
{p: TestPerson} / (p.age < 25 OR p.age > 60) AND p.score > 85.0 ==> profile_junior_senior_expert_success(p.name)
{p: TestPerson} / p.salary < 40000 AND p.score < 70.0 AND p.active == false ==> profile_underperformer_success(p.name)

// === PRODUITS COMPLEXES ===
{prod: TestProduct} / prod.price >= 50.0 AND prod.price <= 200.0 AND prod.rating >= 4.0 AND prod.available == true ==> product_sweet_spot_success(prod.name)
{prod: TestProduct} / (prod.category == "Electronics" OR prod.category == "Tools") AND prod.price > 100.0 ==> product_tech_tools_expensive_success(prod.name)
