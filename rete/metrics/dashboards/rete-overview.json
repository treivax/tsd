{
  "dashboard": {
    "title": "TSD RETE Engine - Overview",
    "tags": ["rete", "tsd", "production"],
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Ingestion Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "rate(tsd_rete_ingestion_total[5m])",
            "legendFormat": "{{mode}} - {{status}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "multi"}
        }
      },
      {
        "id": 2,
        "title": "Network Size",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "tsd_rete_type_nodes",
            "legendFormat": "Type Nodes",
            "refId": "A"
          },
          {
            "expr": "tsd_rete_terminal_nodes",
            "legendFormat": "Terminal Nodes",
            "refId": "B"
          },
          {
            "expr": "tsd_rete_alpha_nodes",
            "legendFormat": "Alpha Nodes",
            "refId": "C"
          },
          {
            "expr": "tsd_rete_beta_nodes",
            "legendFormat": "Beta Nodes",
            "refId": "D"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "auto"
        }
      },
      {
        "id": 3,
        "title": "Ingestion Duration by Phase (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(tsd_rete_ingestion_duration_seconds_bucket[5m])) by (phase, le))",
            "legendFormat": "{{phase}} (p95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Facts Throughput",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "rate(tsd_rete_facts_submitted_total[5m])",
            "legendFormat": "Submitted/s",
            "refId": "A"
          },
          {
            "expr": "rate(tsd_rete_facts_propagated_total[5m])",
            "legendFormat": "Propagated/s",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Facts/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Efficiency Score",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "tsd_rete_efficiency_score",
            "legendFormat": "Efficiency",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.8, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Bottleneck Phases (%)",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "tsd_rete_bottleneck_phase_percentage",
            "legendFormat": "{{phase}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["value"]},
          "pieType": "pie",
          "displayLabels": ["percent"]
        }
      },
      {
        "id": 7,
        "title": "Rule Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "rate(tsd_performance_rule_evaluations_total[5m])",
            "legendFormat": "Evaluations/s",
            "refId": "A"
          },
          {
            "expr": "rate(tsd_performance_rule_matches_total[5m])",
            "legendFormat": "Matches/s",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Transaction Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "rate(tsd_rete_transaction_total[5m])",
            "legendFormat": "{{operation}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Transactions/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Storage Facts",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "tsd_storage_facts_total",
            "legendFormat": "Total Facts",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "auto"
        }
      },
      {
        "id": 10,
        "title": "Coherence Violations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "rate(tsd_coherence_violations_total[5m])",
            "legendFormat": "{{type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Violations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Coherence Mode",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "tsd_coherence_mode",
            "legendFormat": "{{mode}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "options": {"1": {"text": "Strong", "color": "green"}}},
              {"type": "value", "options": {"2": {"text": "Relaxed", "color": "yellow"}}},
              {"type": "value", "options": {"3": {"text": "Eventual", "color": "blue"}}}
            ]
          }
        }
      },
      {
        "id": 12,
        "title": "Transaction Duration (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(tsd_rete_transaction_duration_seconds_bucket[5m])) by (operation, le))",
            "legendFormat": "{{operation}} (p95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 13,
        "title": "Storage Operations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48},
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "targets": [
          {
            "expr": "rate(tsd_storage_operations_total[5m])",
            "legendFormat": "{{operation}} - {{type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Operations/sec"},
          {"format": "short"}
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "DS_PROMETHEUS",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        }
      ]
    }
  }
}
