// Test des actions avec variables entières

type TestUser : <
    id: string,
    name: string,
    age: number
>

// Test avec variable entière comme argument
{u: TestUser} / u.age > 18 ==> process_adult_user(u)

// Test avec mélange variable et champ
{u: TestUser} / u.name == "Alice" ==> process_user_mixed(u, u.id)

