// Test NotNode complex - NÃ©gations complexes avec EXISTS
type Student : <id: string, name: string, major: string, gpa: number>
type Enrollment : <id: string, student_id: string, course_id: string, grade: string>
type Course : <id: string, name: string, credits: number, difficulty: string>

{s: Student} / s.gpa >= 3.5 AND NOT (EXISTS (e: Enrollment / e.student_id == s.id AND e.grade == "F")) ==> dean_list_candidate(s.id)
{s: Student} / s.major == "computer_science" AND NOT (EXISTS (e: Enrollment, c: Course / e.student_id == s.id AND e.course_id == c.id AND c.difficulty == "advanced")) ==> needs_advanced_courses(s.id)  
{s: Student} / NOT (s.gpa < 2.0) AND NOT (EXISTS (e: Enrollment / e.student_id == s.id AND e.grade == "I")) ==> good_standing(s.id)